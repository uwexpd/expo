<thead>
	<tr>
		<th>Student Number</th>
		<th>First Name</th>
		<th>Last Name</th>
		<th>E-mail</th>
		<th>Majors</th>
		<th>Minors</th>
		<th>Class</th>
		<th>Special Program</th>
		<th>Under-represented?</th>
		<th>Ethnicity</th>
		<th>Heritage</th>
		<th>Age</th>
		<th>Gender</th>
		<th>High School</th>
		<th>Transfer Student</th>
		<th>Last Institution</th>
		<th>GPA</th>
	</tr>
</thead>

<tbody>
<% @students.each do |s| %>
	<tr>
		<td><%=h s.student_no %></td>
		<td><%=h s.firstname %></td>
		<td><%=h s.lastname %></td>
		<td><%=h s.email %></td>
		<td><%=h s.majors_list %></td>
		<td><%=h s.minors_list %></td>
		<td><%=h s.class_standing_description %></td>
		<td><%=h s.sdb.special_program.description %></td>
		<td><%=h s.sdb.ethnicity.under_represented? %></td>
		<td><%=h s.sdb.ethnicity.description %></td>
		<td><%=h s.sdb.ethnicity.long_description %></td>
		<td><%=h s.age %></td>
		<td><%=h s.gender %></td>
		<td><%=h s.sdb.high_school.name rescue nil %></td>
		<td><%=h s.transfer_student? %></td>
		<td><%=h s.sdb.last_institution.name rescue nil %></td>
		<td><%=h s.sdb.gpa %></td>
	</tr>
<% end %>
</tbody>