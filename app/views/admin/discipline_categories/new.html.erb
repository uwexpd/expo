<h1>New Discipline Category</h1>

<% upload_form_for :discipline_category, @discipline_category, :url => discipline_categories_path do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "fields", :locals => { :f => f } %>
	
	<%= f.submit "Create", :disable_with => 'Creating...' %>
	<%= separator "or" %>
	<%= link_to 'Cancel', discipline_categories_path, :class => 'cancel button' %>

<% end %>
