<h1>New Equipment</h1>

<% upload_form_for :equipment, @equipment, :url => equipments_path do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "fields", :locals => { :f => f } %>
	
	<%= f.submit "Create", :disable_with => 'Creating...' %>
	<%= separator "or" %>
	<%= link_to 'Cancel', equipments_path, :class => 'cancel button' %>

<% end %>
