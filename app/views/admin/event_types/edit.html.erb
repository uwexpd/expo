<h1>Edit Event Type</h1>

<% form_for :event_type, @event_type, 
			:url => event_type_path(@event_type), 
			:html => { :method => :put } do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "fields", :locals => { :f => f } %>
	
	<%= f.submit "Save", :disable_with => 'Saving...' %>
	<%= separator "or" %>
	<%= link_to 'Cancel', event_type_path(@event_type), :class => 'cancel button' %>

<% end %>
