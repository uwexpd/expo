<table width="100%">

	<%= render :partial => "results_table_header" %>
	
	<% for attendee in auto_complete %>
		<%= render :partial => "attendee", :object => attendee, :locals => { :highlight_phrase => highlight_phrase } %>
	<% end -%>
	
	<% if auto_complete.empty? %>
	<tr class="empty">
		<td colspan=10>Could not find any students with that name</td>
	</tr>
	<% end -%>
	
</table>