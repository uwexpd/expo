<h1>New Sub-time</h1>

<% form_for :sub_time, @sub_time, :url => event_time_sub_times_path(@event, @time) do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "admin/events/times/fields", 
				:locals => { 
					:f => f, 
					:default_start_time => @time.start_time,
					:default_end_time => @time.end_time
					 } %>
	
	<%= f.submit "Create", :disable_with => 'Creating...' %>
	<%= separator "or" %>
	<%= link_to 'Cancel', event_time_sub_times_path(@event, @time) %>

<% end %>
