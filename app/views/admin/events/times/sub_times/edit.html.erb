<h1>Editing Sub-time</h1>

<% form_for :sub_time, @sub_time, :url => event_time_sub_time_path(@event, @time, @sub_time), :html => { :method => :put } do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "admin/events/times/fields", 
				:locals => { 
					:f => f, 
					:default_start_time => @time.start_time,
					:default_end_time => @time.end_time
					 } %>
	
	<%= f.submit "Save", :disable_with => 'Saving...' %>
	<%= separator "or" %>
	<%= link_to 'Cancel', event_time_sub_time_path(@event, @time, @sub_time) %>

<% end %>
