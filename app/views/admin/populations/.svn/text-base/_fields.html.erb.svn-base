<dl class="inline-definitions">
	
	<dt><%= f.label :title %></dt>
	<dd><%= f.text_field :title, :style => "width: 95%" %></dd>
	
	<dt><%= f.label :description %></dt>
	<dd><%= f.text_area :description, :rows => 4, :style => "width: 95%" %></dd>

	<dt><%= f.label :access_level, "Share with" %></dt>
	<dd><%= f.select :access_level, [["Everyone", "everyone"], ["Just Me", "creator"], ["My program(s) staff", "unit"]] %></dd>

	
</dl>

<%= render :partial => "starting_set", :locals => { :f => f } unless @population.new_record? || @population.is_a?(ManualPopulation) %>