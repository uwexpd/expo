<h1>Student Survey Information</h1>
<h3><%= @time.event.title %> on <%= @time.time_detail %></h3>

<table>
  <tr>
    <th>Student</th>
    <th>Birthday</th>
    <th>How did you hear?</th>
    <th>To fulfill MIT?</th>
    <th>Pursue ELS?</th>
    <th>ELS Minor?</th>
    <th>Teaching Career?</th>
    <th>Apply Masters?</th>
  </tr>
  
  <% for attendee in @attendees %>
  <% unless attendee.person.nil? || attendee.person.pipeline_student_info.nil? %>
  <tr>
    <td><%= link_to attendee.person.fullname, admin_student_path(attendee.person) %></td>
    <td><%= attendee.person.sdb.birth_dt.strftime('%m/%d/%Y') %></td>
    <td><%= attendee.person.pipeline_student_info.how_did_you_hear %></td>
    <td><%= attendee.person.pipeline_student_info.fulfill_mit %></td>
    <td><%= attendee.person.pipeline_student_info.pursue_els %></td>
    <td><%= attendee.person.pipeline_student_info.current_els_minor %></td>
    <td><%= attendee.person.pipeline_student_info.teaching_career %></td>
    <td><%= attendee.person.pipeline_student_info.apply_masters %></td>
  </tr>
  <% end %>
  <% end %>
</table>

<% sidebar :pipeline_links %>