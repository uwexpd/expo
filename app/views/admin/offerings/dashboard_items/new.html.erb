<h1>New Dashboard Item</h1>

<% form_for :dashboard_item, @dashboard_item, :url => offering_dashboard_items_path(@offering) do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "fields", :locals => { :f => f } %>
	
	<%= f.submit "Create", :disable_with => 'Creating...' %>
	<%= separator "or" %>
	<%= link_to 'Cancel', offering_dashboard_items_path(@offering) %>

<% end %>
