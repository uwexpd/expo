<h1>New Application Type</h1>

<% form_for :application_type, @application_type, :url => offering_application_types_path(@offering) do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "fields", :locals => { :f => f } %>
	
	<%= f.submit "Create", :disable_with => 'Creating...' %>
	<%= separator "or" %>
	<%= link_to 'Cancel', offering_application_types_path(@offering), :class => 'cancel button' %>

<% end %>
