<h1>New Application Category</h1>

<% form_for :application_category, @application_category, :url => offering_application_categories_path(@offering) do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "fields", :locals => { :f => f } %>
	
	<%= f.submit "Create", :disable_with => 'Creating...' %>
	<%= separator "or" %>
	<%= link_to 'Cancel', offering_application_categories_path(@offering), :class => 'cancel button' %>

<% end %>
