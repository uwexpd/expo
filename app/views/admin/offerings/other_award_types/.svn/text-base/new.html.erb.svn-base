<h1>New Other Award Type</h1>

<% form_for :other_award_type, @other_award_type, :url => offering_other_award_types_path(@offering) do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "fields", :locals => { :f => f } %>
	
	<%= f.submit "Create", :disable_with => 'Creating...' %>
	<%= separator "or" %>
	<%= link_to 'Cancel', offering_other_award_types_path(@offering), :class => 'cancel button' %>

<% end %>
