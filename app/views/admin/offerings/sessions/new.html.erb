<h1>New Session</h1>

<% form_for :session, @session, :url => offering_sessions_path(@offering) do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "fields", :locals => { :f => f } %>
	
	<%= f.submit "Create", :disable_with => 'Creating...' %>
	<%= separator %>
	<%#= link_to 'Cancel', offerings_path(@offering) %>

<% end %>
