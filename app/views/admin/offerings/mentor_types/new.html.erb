<h1>New Mentor Type</h1>

<% form_for :mentor_type, @mentor_type, :url => offering_mentor_types_path(@offering) do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "fields", :locals => { :f => f } %>
	
	<%= f.submit "Create", :disable_with => 'Creating...' %>
	<%= separator "or" %>
	<%= link_to 'Cancel', offering_mentor_types_path(@offering), :class => 'cancel button' %>

<% end %>
