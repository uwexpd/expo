<h1>New Committee Quarter
	<span class="light">&raquo; <%= @committee.name %></span></h1>
	
<% form_for [:admin, @quarter] do |f| %>
	<%= f.error_messages %>
	
	<dl class="inline-definitions">
		<%= render :partial => "fields", :locals => { :f => f } %>
	</dl>
	
	<%= f.submit "Create", :disable_with => "Creating..." %>
	
<% end -%>
 