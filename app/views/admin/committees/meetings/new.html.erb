<h1>New Committee Meeting</h1>
	
<% form_for [:admin, @meeting], :url => admin_committee_meetings_url(@committee) do |f| %>
	<%= f.error_messages %>
	
	<dl class="inline-definitions">
		<%= render :partial => "fields", :locals => { :f => f } %>
	</dl>
		
	<%= f.submit "Create", :disable_with => "Creating..." %>
	
<% end -%>

