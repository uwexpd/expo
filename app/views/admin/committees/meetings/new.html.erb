<h1>New Committee <%= !@committee.meetings_alt_title.blank? ? @committee.meetings_alt_title : 'Meeting' %></h1>
	
<% form_for :meeting, @meeting, :url => admin_committee_meetings_url(@committee) do |f| %>
	<%= f.error_messages %>
	
	<dl class="inline-definitions">
		<%= render :partial => "fields", :locals => { :f => f } %>
	</dl>
		
	<%= f.submit "Create", :disable_with => "Creating..." %>
	
<% end -%>

