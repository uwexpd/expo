<h1>New Equipment Category</h1>

<% upload_form_for :equipment_category, @equipment_category, :url => equipment_categories_path do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "fields", :locals => { :f => f } %>
	
	<%= f.submit "Create", :disable_with => 'Creating...' %>
	<%= separator "or" %>
	<%= link_to 'Cancel', equipment_categories_path, :class => 'cancel button' %>

<% end %>
