<h1>Edit General Study Faculty</h1>

<% form_for :general_study_faculty, @general_study_faculty, 
			:url => general_study_faculty_path(@general_study_faculty), 
			:html => { :method => :put } do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "fields", :locals => { :f => f } %>
	
	<%= f.submit "Save", :disable_with => 'Saving...' %>
	<%= separator "or" %>
	<%= link_to 'Cancel', general_study_faculty_path(@general_study_faculty), :class => 'cancel button' %>

<% end %>
