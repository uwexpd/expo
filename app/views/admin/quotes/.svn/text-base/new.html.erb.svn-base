<h1>New Quote</h1>

<% upload_form_for :quote, @quote, :url => quotes_path do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "fields", :locals => { :f => f } %>
	
	<%= f.submit "Create", :disable_with => 'Creating...' %>
	<%= separator "or" %>
	<%= link_to 'Cancel', quotes_path, :class => 'cancel button' %>

<% end %>
