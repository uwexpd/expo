<h1>New Mentor</h1>

<% upload_form_for :mentor, @mentor, :url => admin_apply_mentors_path(@offering, @app) do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "fields", :locals => { :f => f } %>
	
	<%= f.submit "Create", :disable_with => 'Creating...' %>
	<%= separator "or" %>
	<%= link_to 'Cancel', admin_apply_mentors_path(@offering, @app) %>

<% end %>
