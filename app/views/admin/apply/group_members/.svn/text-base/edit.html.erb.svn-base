<h1>Editing Group_member</h1>

<% form_for :group_member, @group_member, :url => { :action => 'update', :id => @group_member }, :html => { :method => :put } do |f| %>

	<%= f.error_messages %>

	<%= render :partial => "fields", :locals => { :f => f } %>
	
	<%= f.submit "Save", :disable_with => 'Saving...' %>
	<%= separator "or" %>
	<%= link_to 'Cancel', :action => 'index' %>

<% end %>
