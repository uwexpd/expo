<% quote = Quote.key("proceedings_students_#{@offering.id.to_s}").random %>

<% unless quote.empty?; quote = quote.first %>
	<div class="student_quotes box grey" id="student_quotes">

		<h2>Student Voices</h2>

		<span class="quotation_mark">&ldquo;</span>
		<p><%= quote.quote %></p>
	
	</div>
<% end -%>